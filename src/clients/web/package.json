{
    "name": "rustscape-web-client",
    "version": "1.0.0",
    "description": "Web-based client for Rustscape game server",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint src --ext .ts,.tsx",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:run": "vitest run",
        "test:ws": "node scripts/test-websocket.js",
        "test:ws:verbose": "node scripts/test-websocket.js localhost 43596"
    },
    "dependencies": {
        "pako": "^2.1.0",
        "seek-bzip": "^2.0.0",
        "lzma": "^2.3.2",
        "pixi.js": "^8.0.0",
        "@pixi/sound": "^6.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/pako": "^2.0.3",
        "@pixi/devtools": "^2.0.0",
        "typescript": "^5.3.0",
        "vite": "^5.0.0",
        "vitest": "^1.0.0",
        "ws": "^8.14.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "runescape",
        "rustscape",
        "game",
        "client",
        "websocket"
    ],
    "author": "rustscape",
    "license": "MIT"
}
